{
  "name": "@logixode/force-graph-lib",
  "private": false,
  "version": "0.1.6",
  "description": "Force-directed graph visualization library.",
  "author": "Rohmad Kurniadi",
  "license": "MIT",
  "homepage": "https://logixode.github.io/force-graph-lib/introduction.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logixode/force-graph-lib.git"
  },
  "keywords": [
    "force-graph",
    "graph",
    "visualization",
    "d3",
    "typescript"
  ],
  "type": "module",
  "files": [
    "dist/force-graph-lib.js",
    "dist/force-graph-lib.umd.cjs",
    "dist/index.d.ts"
  ],
  "main": "./dist/force-graph-lib.umd.cjs",
  "module": "./dist/force-graph-lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/force-graph-lib.js",
      "require": "./dist/force-graph-lib.umd.cjs"
    }
  },
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build && npx tsc --project tsconfig.lib.json --noEmit",
    "preview": "bunx --bun vite preview",
    "docs:dev": "bunx --bun vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "bunx --bun vitepress preview"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@tanstack/vue-query": "^5.87.1",
    "@types/node": "^24.3.1",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "json-editor-vue": "^0.18.1",
    "jsoneditor": "^10.4.1",
    "lucide-vue-next": "^0.543.0",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^2.0.0-alpha.12",
    "vue-sonner": "^2.0.8"
  },
  "devDependencies": {
    "d3": "^7.9.0",
    "force-graph": "^1.50.1",
    "@types/d3": "^7.4.3",
    "d3-force-clustering": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "3",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  },
  "overrides": {
    "ajv": "^8.12.0"
  }
}
